<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginTop="@dimen/tof_text_size"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/LinearLayout01"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1.0"
        android:orientation="vertical" >

        <TableRow
            android:id="@+id/TableRow01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/status"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/statusTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/voltage"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/voltageTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/battery_capacity"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/fullcapacityTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/plugged"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/pluggedTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>
        
        <TableRow
            android:id="@+id/TableRow09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/typec_forward_charging"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/type_c_forward_charging"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/typec_forward_charging_text"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>
        
        <TableRow
            android:id="@+id/TableRow10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/typec_backward_charging"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/type_c_backward_charging"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/typec_backward_charging_text"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" >

            <TextView
                android:id="@+id/electronicnoteTV"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/charger_Voltage"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/electronicTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" >

            <TextView
                android:id="@+id/isQuickChargeTV"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/is_quick_charge"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/isQuickChargeTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/battery_level_tv"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/battery_level"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/battery_level"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/battery_charging_status_tv"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/battery_charing_status"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/battery_charging_status"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" >

            <TextView
                android:id="@+id/batteryelectronicnoteTV"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/charger_electronic"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/batteryelectronicTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <!-- SPRD Bug 773805:Show NTC temperature in charger test. @{ -->
        <TableRow
            android:id="@+id/TableRow06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone">

            <TextView
                android:id="@+id/batterytemperature"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ntc_temperature"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/batterytemperatureTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>
        <!-- @} -->
        <!-- SPRD Bug 850012:Show battery type id in charger test. @{ -->
        <TableRow
            android:id="@+id/TableRow08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone">

            <TextView
                android:id="@+id/battery_type_id"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/battery_type_id"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/battery_type_idextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>
        <!-- @} -->
	<TableRow
            android:id="@+id/TableRow07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TextView
                android:id="@+id/test_resultTextView"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:text="@string/charging_test"
                android:paddingLeft="10px"
                android:visibility="gone"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>
    </LinearLayout>

</LinearLayout>
